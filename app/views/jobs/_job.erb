<tr id="tr<%=job.id%>">
<td><%= link_to_remote(image_tag("edit.gif"), :url=>{:controller=>'jobs', :action=>'edit_form', :id=>job.id}, :loading=>"$('edit').update('Loading...');$('edit').top=this.top;$('edit').show();", :update=>'edit') %></td>
<td><%= job.date %></td>
<td><%= job.employee.team %> <%= job.employee.name %></td>
<td><%= job.zone.code %> <%= job.zone.name %></td>
<td><%= job.serial.serial %></td>
<td><%= job.phone.phone %></td>
<td><%= job.job_type.name %></td>
<td><%= job.friendly_result %></td>
<td><%= job.ampm %></td>
<td><%= h job.memo %></td>
</tr>

